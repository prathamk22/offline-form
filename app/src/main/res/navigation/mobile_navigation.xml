<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.offline.form.builder.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_nav_gallery"
            app:destination="@id/nav_gallery" />
        <action
            android:id="@+id/action_nav_home_to_houseHoldAssetsFragment"
            app:destination="@id/houseHoldAssetsFragment" />
        <action
            android:id="@+id/action_nav_home_to_farmerGroupAssociation"
            app:destination="@id/farmerGroupAssociation" />
        <action
            android:id="@+id/action_nav_home_to_cropProfileFragment"
            app:destination="@id/cropProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.offline.form.builder.ui.gallery.AddPeopleFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.offline.form.builder.ui.allData.AllFormDataFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/houseHoldAssetsFragment"
        android:name="com.offline.form.builder.ui.HouseHoldAssetsFragment"
        android:label="HouseHoldAssetsFragment" />
    <fragment
        android:id="@+id/farmerGroupAssociation"
        android:name="com.offline.form.builder.ui.FarmerGroupAssociation"
        android:label="FarmerGroupAssociation" />
    <fragment
        android:id="@+id/cropProfileFragment"
        android:name="com.offline.form.builder.ui.CropProfileFragment"
        android:label="CropProfileFragment" />
</navigation>